{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "net.tagaini.kanjivg.focusviewer",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/desktop-dist/",
  "url": "/desktop-viewer.html",
  "enableServer": true,
  "enableNativeAPI": false,
  "tokenSecurity": "none",
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  },
  "modes": {
    "window": {
      "title": "KanjiVG Focus Viewer",
      "width": 1360,
      "height": 900,
      "minWidth": 900,
      "minHeight": 620,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "kanjivg-focus-viewer",
    "resourcesPath": "/desktop-dist/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/js/neutralino.js",
    "binaryVersion": "6.5.0",
    "clientVersion": "6.5.0"
  }
}
